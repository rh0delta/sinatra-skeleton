

<h2>All Tracks</h2>
<hr>
<div class="container-fluid"> 
  <div class = "row">
    <% if @songs %>
      <% @songs.each do |song| %>
        <div class="col-sm-3 col-md-3">
          <h3><a href="/songs/<%= song.id %>"><%= song.title %></a></h3>    
          <p>
            By: <%= song.author %>
          </p>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
